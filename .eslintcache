rukere.map(bruker => {\r\n        return(\r\n            <div className=\"col width-margin m6 card-panel nominerKort\" key= {bruker.id} >\r\n                <p ref={ fornavnRef } > { bruker.Fornavn } </p>\r\n                <p ref={ etternavnRef } > { bruker.Etternavn } </p>\r\n                { !harStemt && <button className=\"float-right btn waves-effect waves-light\" onClick= { () => handleSubmit(bruker.id) } >Stem</button>}\r\n                { harStemt && <button className=\"float-right btn waves-effect waves-light\" onClick= { () => handleSubmit(bruker.id) } disabled >Stem</button>}\r\n            </div>\r\n        )}\r\n        )\r\n        }\r\n        </div>\r\n    </div>\r\n     );\r\n}\r\n \r\n","C:\\Users\\Seb\\Documents\\GitHub\\React-Utviklingsoppgave-\\src\\components\\Logginn.js",[],"C:\\Users\\Seb\\Documents\\GitHub\\React-Utviklingsoppgave-\\src\\components\\BrukerProfil.js",["68"],"C:\\Users\\Seb\\Documents\\GitHub\\React-Utviklingsoppgave-\\src\\components\\Registrering.js",[],"C:\\Users\\Seb\\Documents\\GitHub\\React-Utviklingsoppgave-\\src\\context\\authContext.js",[],"C:\\Users\\Seb\\Documents\\GitHub\\React-Utviklingsoppgave-\\src\\server\\firebase.js",[],{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","severity":1,"message":"74","line":50,"column":15,"nodeType":"75","endLine":50,"endColumn":80},{"ruleId":"76","severity":1,"message":"77","line":4,"column":19,"nodeType":"78","messageId":"79","endLine":4,"endColumn":29},{"ruleId":"76","severity":1,"message":"80","line":4,"column":31,"nodeType":"78","messageId":"79","endLine":4,"endColumn":41},{"ruleId":"73","severity":1,"message":"81","line":46,"column":32,"nodeType":"75","endLine":46,"endColumn":55},{"ruleId":"82","severity":1,"message":"83","line":36,"column":8,"nodeType":"84","endLine":36,"endColumn":10,"suggestions":"85"},{"ruleId":"76","severity":1,"message":"86","line":30,"column":14,"nodeType":"78","messageId":"79","endLine":30,"endColumn":28},"no-nativ